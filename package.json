{
  "name": "gdpr_for_developers",
  "version": "0.0.0",
  "private": true,
  "description": "gdpr-for-developers",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc && vite build --emptyOutDir",
    "dev": "vite",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "preview": "vite preview",
    "start": "vite",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.1.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.29",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/ws": "8.5.3",
    "@vitejs/plugin-react": "^1.0.7",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-css-modules": "2.1.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.17",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "vite": "2.9.6"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
